{
  "$schema": "https://json.schemastore.org/web-manifest.json",
  "name": "xueyou-react-template",
  "short_name": "xueyou-react-template",
  "description": "react + react-router + ssr + pnpm + ts + rsbuild",
  "icons": [
    {
      "src": "{BASE_URL}icons/128x128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "{BASE_URL}icons/512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "{BASE_URL}icons/512x512.png",
      "type": "image/png",
      "sizes": "512x512"
    },
    {
      "src": "{BASE_URL}screenshot1-wide.jpg",
      "type": "image/jpeg",
      "sizes": "1126x497",
      "form_factor": "wide"
    }
  ],
  "start_url": "{BASE_URL}",
  "scope": "{BASE_URL}",
  "display": "fullscreen",
  "background_color": "#000000",
  "theme_color": "#4DBA87",
  "orientation": "landscape-primary"
}
