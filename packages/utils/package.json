{
  "name": "@packages/utils",
  "version": "0.0.1",
  "description": "React Utils",
  "author": "XueYou",
  "license": "ISC",
  "packageManager": "pnpm@9.0.0",
  "types": "./dist/esm/index.d.ts",
  "main": "./src/index.ts",
  "module": "./dist/esm/index.mjs",
  "exports": {
    ".": {
      "node": {
        "types": "./dist/cjs/index.d.ts",
        "import": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "import": "./src/index.ts"
      },
      "default": {
        "types": "./dist/esm/index.d.ts",
        "import": "./dist/esm/index.js"
      }
    }
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "files": [
    "lib",
    "dist",
    "esm",
    "README.md"
  ],
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint ./**/*.{scss,css,less,sass}",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "build": "echo 'rslib build'",
    "dev": "echo 'rslib build --watch'"
  },
  "keywords": [
    "React",
    "Utils"
  ],
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.6.7"
  },
  "stylelint": {
    "extends": "../../framework/lint-set/stylelint.config.mjs",
    "ignorePath": "../../framework/lint-set/.stylelintignore"
  },
  "devDependencies": {
    "@framework/build": "workspace:^",
    "@framework/lint-set": "workspace:^",
    "@rsbuild/plugin-react": "^1.1.0",
    "@rsbuild/plugin-sass": "^1.1.2",
    "@rslib/core": "^0.3.1",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.2"
  }
}
